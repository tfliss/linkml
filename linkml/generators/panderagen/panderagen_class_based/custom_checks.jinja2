{%- macro render_custom_check(field) -%}
    {%- with slot = field.source_slot -%}
        {%- if slot.minimum_cardinality or slot.maximum_cardinality %}
    @pla.check("{{ field.name }}")
    def check_cardinality_{{ field.name }}(cls, data: PolarsData):
        return cls._check_cardinality(
            data.lazyframe,
            data.key,
            min_cardinality={{ slot.minimum_cardinality }},
            max_cardinality={{ slot.maximum_cardinality }}
        )
        {%- endif -%}
        {%- if slot.annotations['reference_class'] and slot.annotations['inline_form'] != 'simple_dict' %}
    @pla.check("{{ field.name }}")
    def check_nested_struct_{{ field.name }}(cls, data: PolarsData):
        {% if slot.annotations['inline_form'] == 'inline_list_dict' %}
        return cls._check_nested_struct({{ slot.annotations['reference_class'] }}, data)
        {% elif slot.annotations['inline_form'] == 'inline_collection_dict' %}
        return cls._check_collection_struct({{ slot.annotations['reference_class'] }}, data)
        {% else %}
        return cls._check_nested_list_struct({{ slot.annotations['reference_class'] }}, data)
        {% endif %}
        {%- endif -%}
        {%- if slot.annotations['reference_class'] and slot.annotations['inline_form'] == 'simple_dict' %}
    @pla.check("{{ field.name }}")
    def check_simple_dict_{{ field.name }}(cls, data: PolarsData):
        return cls._check_simple_dict({{ slot.annotations['reference_class'] }}, data)
        {%- endif -%}
    {%- endwith -%}
{%- endmacro -%}
